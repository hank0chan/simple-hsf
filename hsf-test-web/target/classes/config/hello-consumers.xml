<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="helloServiceConsumer" class="com.taobao.hsf.app.spring.util.HSFSpringConsumerBean" init-method="init">
		<property name="interfaceName" value="test.apis.HelloService"></property>
		<property name="version" value="1.0.0"></property>
		<property name="group" value="TEST_HSF"></property>
		<property name="clientTimeout" value="300000"></property>
		<property name="maxWaitTimeForCsAddress" value="30000"></property>
	</bean>

</beans>